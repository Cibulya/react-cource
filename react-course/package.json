        "express": "^4.18.2",
        "msw": "^1.2.1",
        "nodemon": "^2.0.22",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.43.9",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.9.0",
        "sirv": "^2.0.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/compression": "^1.7.2",
