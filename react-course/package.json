{
    "name": "react-course",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "ts-node --esm  server.ts",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": " eslint \"**/*.{ts,tsx}\" --fix",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "lintCheck": "npx eslint src",
        "cy:open-e2e": "cypress open --e2e --browser chrome",
